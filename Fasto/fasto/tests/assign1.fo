fun int mul(int x, int y) =
  if x == 0 
  then 0 
  else 
    if y == 0 
    then 0 
    else 
      if 0 < x 
      then
        if 0 < y 
        then mul(0 - x, 0 - y) 
        else 0 - mul(0 - x, y)
      else 
        if 0 < y
        then 0 - mul(x, 0 - y)
        else x + mul(x, y - 1)
        

fun int readInt(int i) = read(int)

fun int mulself(int x) = mul(x, x)

fun int dist([int] arr, int i) =
    if i == 0
    then arr[0]
    else arr[i] - arr[i-1]

fun int add(int x, int y) = x + y

fun int main() =
    let n = read(int) in
      if 0 < n
      then
          let arr = map(readInt, iota(n)) in
          let difs = map(fn int (int i) => dist(arr,i), iota(n)) in
          let sqrs = map(mulself, difs) in
          reduce(add, 0, sqrs)
      else 
          0
        
